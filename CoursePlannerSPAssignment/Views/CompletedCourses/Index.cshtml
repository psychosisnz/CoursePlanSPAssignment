@model IEnumerable<CoursePlannerSPAssignment.Models.CompletedCourse>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Course.CourseCode)
        </th>
        @*<th>
            @Html.DisplayNameFor(model => model.Student.FName)
        </th>*@
        <th>
            @Html.DisplayNameFor(model => model.Passed)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.YearCompleted)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SemesterCompleted)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Course.CourseCode)
        </td>
        @*<td>
            @Html.DisplayFor(modelItem => item.Student.FName)
        </td>*@
        <td>
            @Html.DisplayFor(modelItem => item.Passed)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.YearCompleted)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SemesterCompleted)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.CompleteId }) |
            @Html.ActionLink("Details", "Details", new { id=item.CompleteId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.CompleteId })
        </td>
    </tr>
}

</table>
